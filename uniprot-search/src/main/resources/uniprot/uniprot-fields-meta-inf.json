{
  "meta-inf": {
    "reference": "https://google.github.io/styleguide/jsoncstyleguide.xml",
    "comment1": "Google advocates flattening structured data by default unless useful, and camelcase attribute naming."
    "comment2": "Derived from result_field.json, UniProtField, field_map.json and uniprot_search.json."
  },
  "searchInfo": [
    {
      "searchEngineField": "accession",
      "regex" : "([O,P,Q][0-9][A-Z|0-9]{3}[0-9]|[A-N,R-Z]([0-9][A-Z][A-Z|0-9]{2}){1,2}[0-9])(-\\d+)*",
      "fieldType" : "TERM",
      "id": "id_accession",
      "label": "UniProtKB AC",
      "itemType": "single",
      "dataType": "string",
      "description": "Search by UniProtKB Accession",
      "example": "P12345"
    },
    {
      "searchEngineField": "gene",
      "searchEngineSortField": "gene_sort",
      "id": "id_gene",
      "label": "Gene Name [GN]",
      "dataType": "string",
      "description": "Search by UniProtKB gene name",
      "example": "ydj1"
    },
    {
      "searchEngineField": "gene_exact",
      "fieldType" : "TERM"
    },
    {
      "searchEngineField": "existence",
      "id": "id_existence",
      "label": "Protein Existence [PE]",
      "dataType": "enum",
      "description": "Search by protein existence",
      "values": [
        {
          "name": "Evidence at protein level",
          "value": "protein_level"
        },
        {
          "name": "Evidence at transcript level",
          "value": "transcript_level"
        },
        {
          "name": "Inferred from homology",
          "value": "homology"
        },
        {
          "name": "Predicted",
          "value": "predicted"
        },
        {
          "name": "Uncertain",
          "value": "uncertain"
        }
      ],
      "example": "1"
    },
    {
      "id": "id_host",
      "label": "Virus host",
      "searchEngineField": "host",
      "dataType": "string",
      "autoComplete": "/uniprot/api/suggester?dict=host&query=?",
      "description": "Search by virus host",
      "example": "human",
      "termSuffix": true
    },
    {
      "id": "id_organism",
      "label": "Organism [OS]",
      "searchEngineField": "organism",
      "dataType": "string",
      "autoComplete": "/uniprot/api/suggester?dict=organism&query=?",
      "description": "Search by Organism name",
      "example": "saccharomyces",
      "termSuffix": true
    },
    {
      "id": "id_taxonomy",
      "label": "Taxonomy [OC]",
      "searchEngineField": "taxonomy",
      "dataType": "string",
      "autoComplete": "/uniprot/api/suggester?dict=taxonomy&query=?",
      "description": "Search by NCBI taxonomy",
      "example": "human",
      "termSuffix": true
    },
    {
      "id": "id_ec",
      "label": "Function|Enzyme classification [EC]",
      "term": "ec",
      "dataType": "string",
      "autoComplete": "/uniprot/api/suggester?dict=ec&query=?",
      "description": "Search by Enzyme EC number",
      "example": "1.1.2.3"
    },
    {
      "id": "id_cofactor_chebi",
      "label": "Function|Cofactors|ChEBI term",
      "itemType": "comment",
      "term": "cofactor_chebi",
      "dataType": "string",
      "autoComplete": "/uniprot/api/suggester?dict=chebi&query=?",
      "hasEvidence": true,
      "description": "Search by cofactor chebi ",
      "example": "29105"
    },
    {
      "id": "id_cofactor_note",
      "label": "Function|Cofactors|Note",
      "itemType": "comment",
      "term": "cofactor_note",
      "dataType": "string",
      "hasEvidence": true,
      "description": "Search by cofactor note",
      "example": "subunit"
    }
  ],
  "resultInfo": [
    {
      "groupName": "Names & Taxonomy",
      "label": "Gene Names",
      "name": "gene_names",
      "paths": [
        "genes[*].geneName.value",
        "genes[*].synonyms[*].value",
        "genes[*].orderedLocusNames[*].value",
        "genes[*].orfNames[*].value"
      ]
    },
    {
      "groupName": "Names & Taxonomy",
      "label": "Gene Names (ordered locus)",
      "name": "gene_oln",
      "paths": [
        "genes[*].orderedLocusNames[*].value"
      ]
    },
    {
      "groupName": "Names & Taxonomy",
      "label": "Gene Names (ORF)",
      "name": "gene_orf",
      "paths": [
        "genes[*].orfNames[*].value"
      ]
    },
    {
      "groupName": "Names & Taxonomy",
      "label": "Gene Names (primary)",
      "name": "gene_primary",
      "paths": [
        "genes[*].geneName.value"
      ]
    },
    {
      "groupName": "Names & Taxonomy",
      "label": "Gene Names (synonym)",
      "name": "gene_synonym",
      "paths": [
        "genes[*].synonyms[*].value"
      ]
    },
    {
      "groupName": "Miscellaneous",
      "label": "Protein existence",
      "name": "protein_existence",
      "paths": [
        "organismHosts[*].scientificName"
      ]
    },
    {
      "groupName": "Names & Taxonomy",
      "label": "Virus hosts",
      "name": "organism_host",
      "paths": [
        "organismHosts[*].scientificName"
      ]
    }
  ]
}